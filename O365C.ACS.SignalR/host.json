{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "Default": "Warning",
      "Host": "Warning",
      "Microsoft": "Warning",
      "Function": "Information",
      "Worker": "Warning",
      "Azure.Storage.Queues": "Warning"
    }
  },
  "extensions": {
    "queues": {
      "messageEncoding": "none"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}